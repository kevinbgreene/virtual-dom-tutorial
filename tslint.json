{
  "extends": "tslint:recommended",
  "rules": {
      "no-console": false,
      "variable-name": false,
      "max-line-length": false,
      "jsdoc-format": false,
      "object-literal-sort-keys": false,
      "no-var-requires": false,
      "array-type": [
          true,
          "generic"
      ],
      "semicolon": [
          true,
          "never"
      ],
      "quotemark": [
          true,
          "single",
          "avoid-escape"
      ]
  }
}